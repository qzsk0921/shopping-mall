  parseCategory: function (list, type) {
    var res
    if (list) {
      if (type === 'even') {
        res = list.filter(function (item, index) {
          return index % 2 === 0
        })
      } else if (type === 'odd') {
        res = list.filter(function (item, index) {
          return index % 2 === 1
        })
      }
    }
    return res
  }
}